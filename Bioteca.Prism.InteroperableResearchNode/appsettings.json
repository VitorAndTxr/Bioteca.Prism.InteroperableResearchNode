{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ChannelSecurity": {
    "KeyExchangeAlgorithm": "ECDH-P384",
    "SupportedCurves": [
      "P384",
      "P256",
      "P521"
    ],
    "SupportedCiphers": [
      "AES-256-GCM",
      "ChaCha20-Poly1305"
    ],
    "ChannelExpirationMinutes": 30,
    "MaxActiveChannels": 100
  },
  "Redis": {
    "ConnectionString": "localhost:6379,abortConnect=false",
    "EnableRedis": false
  },
  "FeatureFlags": {
    "UseRedisForSessions": false,
    "UseRedisForChannels": false
  },
  "HttpClient": {
    "TimeoutSeconds": 300
  },
  "AzureBlobStorage": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "ContainerName": "node-a"
  }
}
